<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.develop.web.domain.personnel.team.mapper.TeamMapper">
    <select id="selectTeamList" resultType="TeamDto" >
        select id, name from team where p_id != 1
    </select>

    <select id="selectTeamTypeList" resultType="TeamDto" >
        select id, name from team where p_id = 1 and id != 1
    </select>
</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.develop.web.domain.admin.rank.mapper.RankMapper">
    <select id="selectListRank" resultType="RankDto">
        SELECT id   AS rankId,
               name AS rankName
        FROM `rank`
        WHERE p_id = 0
          AND id NOT IN (0)
    </select>
</mapper>
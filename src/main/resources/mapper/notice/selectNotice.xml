<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.develop.web.domain.notice.mapper.NoticeMapper">
    <select id="selectNotice" resultType="NoticeDto" parameterType="Integer">
        SELECT
        id, emp_id AS empId, writer, title, content
        FROM notice WHERE id = ${noticeId}
    </select>


    <select id="selectNoticeList" resultType="NoticeDto" >
        SELECT
        id, emp_id AS empId, writer, title, content
        FROM notice
    </select>
</mapper>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.develop.web.domain.notice.mapper.NoticeMapper">
    <insert id="insertNotice" parameterType="PostDto">
        insert
        into notice(emp_id, writer, title, content)
        values (#{empId}, #{writer}, #{title}, #{content})
    </insert>
</mapper>
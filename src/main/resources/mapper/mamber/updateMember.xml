<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.develop.web.domain.users.auth.mapper.AuthMapper">
    <update id="updatePassword" parameterType="String">
        UPDATE emp SET password = #{changePassword} WHERE account = #{account}
    </update>

    <update id="updateMemberProgramId" parameterType="ProgramUpdateParam">
        UPDATE emp SET program_id = #{programId} WHERE account = #{account}
    </update>
</mapper>
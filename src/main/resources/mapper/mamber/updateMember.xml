<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.develop.web.domain.auth.mapper.AuthMapper">
    <update id="updatePassword" parameterType="String">
        UPDATE emp SET password = #{changePassword} WHERE account = #{account}
    </update>

    <update id="updateMemberDeleteFlag" parameterType="String">
        UPDATE emp SET del_fl = 1 WHERE account = #{account}
    </update>
</mapper>
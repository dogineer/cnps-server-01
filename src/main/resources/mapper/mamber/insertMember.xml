<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.develop.web.domain.users.auth.mapper.AuthMapper">
    <insert id="insertUser" parameterType="JoinedUser">
        INSERT
        INTO emp(account,
                 name,
                 gender,
                 phone,
                 email,
                 dept_id,
                 rank_id,
                 des,
                 password)
        VALUES (#{account},
                #{name},
                #{gender},
                #{phone},
                #{email},
                #{deptId},
                #{rankId},
                #{des},
                #{password})
    </insert>
</mapper>